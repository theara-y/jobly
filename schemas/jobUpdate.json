{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "examples": [
                "Developer"
            ]
        },
        "salary": {
            "title": "The salary Schema",
            "type": "integer",
            "examples": [
                70000
            ]
        },
        "equity": {
            "title": "The equity Schema",
            "type": "number",
            "examples": [
                0
            ]
        }
    },
    "examples": [{
        "title": "Developer",
        "salary": 70000,
        "equity": 0
    }]
}