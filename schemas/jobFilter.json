            "examples": [
                70000
            ]
        },
        "hasEquity": {
            "title": "The hasEquity Schema",
            "type": "boolean",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "title": "Developer",
        "minSalary": 70000,
        "hasEquity": true
    }]
}