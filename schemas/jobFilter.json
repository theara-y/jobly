    "title": "Root Schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "examples": [
                "Developer"
            ]
        },
        "minSalary": {
            "title": "The minSalary Schema",
